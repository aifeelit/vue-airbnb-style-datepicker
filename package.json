{
  "name": "vue-airbnb-style-datepicker",
  "version": "1.0.0",
  "description": "A VueJs version of the popular AirBnb datepicker",
  "main": "dist/vue-airbnb-style-datepicker.cjs.js",
  "module": "dist/vue-airbnb-style-datepicker.es.js",
  "unpkg": "dist/vue-airbnb-style-datepicker.js",
  "repository": {
    "url": "MikaelEdebro/vue-airbnb-style-datepicker",
    "type": "git"
  },
  "files": ["dist"],
  "poi": {
    "entry": "dev/index.js",
    "dist": "dev/dist",
    "homepage": "/vue-airbnb-style-datepicker/"
  },
  "scripts": {
    "build":
      "rimraf dist/ && bili --plugin vue --format cjs,umd,umd-min,es --vue.css dist/vue-airbnb-style-datepicker.css",
    "dev": "poi --port 5000"
  },
  "author": "MikaelEdebro <mikael.edebro@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "node-sass": "^4.7.2",
    "poi": "^9.6.13",
    "rimraf": "^2.6.2",
    "rollup-plugin-vue": "^3.0.0",
    "sass-loader": "^6.0.6",
    "vue-template-compiler": "^2.5.13"
  },
  "dependencies": {
    "date-fns": "^1.29.0",
    "vue": "^2.5.13"
  }
}
